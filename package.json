{
  "name": "feishu-md-export",
  "version": "0.2.0",
  "description": "MCP Server & CLI tool to export Feishu/Lark Docs to DocAST + Markdown + assets.",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "feishu-md-export": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "mcp": "tsx src/mcp/server.ts",
    "test": "vitest run",
    "check": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "feishu",
    "lark",
    "markdown",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}